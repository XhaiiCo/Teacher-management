<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">

	<navigation-rule>
		<from-view-id>/loggedUser/*</from-view-id>
		<navigation-case>
			<from-outcome>/DDOM/userCRUD</from-outcome>
			<to-view-id>/loggedUser/DDOM/userCRUD.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/loggedUser/*</from-view-id>
		<navigation-case>
			<from-outcome>/S/teacherCRUD</from-outcome>
			<to-view-id>/loggedUser/S/teacherCRUD.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/loggedUser/*</from-view-id>
		<navigation-case>
			<from-outcome>/index</from-outcome>
			<to-view-id>/loggedUser/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
